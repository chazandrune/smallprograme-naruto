.pop_setting .text_body .li{ position: relative; background-color: rgba(0, 0, 0, .1);  padding: calc(6vmin / 3.75) 10rpx;  margin: 10rpx 0 10rpx; border-radius: 5rpx; overflow: hidden;color: #fff; }
.pop_setting .text_body .li .dl{ width: 70%;}
.pop_setting .text_body .li .dl .dt{ line-height: 22rpx; height: 22rpx; font-weight: bold; font-size: 14rpx;  }
.pop_setting .text_body .li .description{font-size: 10rpx; line-height: 12rpx; padding: 5rpx 0 5rpx;  }
.pop_setting .text_body .li .dl_r{ position: absolute; right: 10rpx; top: 50%; margin-top: -15rpx;}
.pop_setting .text_body .li .dl_r .btn_switch{ margin-left: 10rpx;}
.pop_setting .text_body .li .dl_r .slider_wrap{width: 150rpx; margin-top: 0; margin-right: 0; display: inline-block; vertical-align: middle;}
.pop_setting .text_body .li .dl_r .slider_wrap .volume_slider{ margin: 0 15rpx; height: 30rpx;}
.pop_setting .text_body .li .btns{ float: right; width: auto;}
.pop_setting .text_body .li .btns .btn{ margin: 0; width: 80rpx; height: 30rpx; line-height: 30rpx; font-size: 12rpx;}
.pop_setting .text_body .li .btn_speedup{ overflow: visible;}
.pop_setting .text_body .li .btn_speedup .icon_ad{ width: 18rpx; height: 18rpx; vertical-align: middle; margin-top: -3rpx; left: auto; top: -10rpx; right: -10rpx;}
.pop_setting .text_body .li .dl_r .btn_sound_wrap{ display:inline-block; width: 30rpx; height: 30rpx; }
.pop_setting .text_body .li .dl_r .btn_sound_wrap .btn_mute{ width: 30rpx; height: 30rpx;}
.pop_setting .text_body .li .dl_r .btn_sound_wrap .btn_mute .icon{ width: 18rpx; height: 18rpx; left: 50%; top:50%; margin-left: -9rpx; margin-top: -9rpx;}

.pop_setting .text_body .libtns{ padding:0; height:65rpx; }
.pop_setting .text_body .libtns .btn{ position: relative; display: inline-block; width:60rpx;height: 60rpx; background: none; line-height: 60rpx; text-align: center; color: #fff; font-size: 14rpx; margin-top: 0; }
.pop_setting .text_body .libtns .btn .icon{ position: absolute; left: 50%; top: 50%; width:36rpx; height: 36rpx; margin-left: -18rpx; margin-top:-25rpx; }
.pop_setting .text_body .libtns .btn>.txt{ white-space:nowrap; display:block; width:100%; position:absolute; left:0; right:0; top:38rpx;line-height:20rpx;}
.pop_setting .text_body .libtns .libtns_scroll{ width: 100%; height: 100%; white-space: nowrap;}

.pop .text_body .liad{ padding: 0; background-color: transparent; text-shadow: none;}

/* 设置界面 tab选项卡切换 */
.pop_setting .nr{ position: absolute;left:12rpx; top: 42rpx; bottom:5rpx; right: 12rpx; display: flex;}
.tab_swiper_wrap{  width: 15%; padding: 2rpx 0; text-align: center; vertical-align: middle;}
.tab_swiper_wrap .tab_item{ width: 100%; height: 25%; position: relative; margin: 0; border-radius: 0; border-radius: 10rpx 0 0 10rpx; box-sizing: border-box; border-width: 5rpx 1rpx 5rpx 5rpx; border-color:transparent; border-style: solid;}
.tab_swiper_wrap .tab_item.current{ background-color: rgba(0, 0, 0, .1); border-color:rgba(0, 0, 0, .1) rgba(255, 255, 255, .4) rgba(0, 0, 0, .1) rgba(0, 0, 0, .1);}
.tab_swiper_wrap .tab_item[disabled]{ background: none; filter: grayscale(1); opacity: .3;}
.tab_swiper_wrap .tab_item .icon{ position: absolute; top: 0; left: 50%; margin-left: -14rpx; width: 32rpx; height: 32rpx;}
.tab_swiper_wrap .tab_item .txt{ position: absolute; left:5rpx; right: 0; bottom: 2rpx; height: 14rpx; line-height: 14rpx; white-space: nowrap; font-size: 12rpx; color: #fff; }
.swiper_wrap{ width: 85%; height: 100%; background-color: rgba(0, 0, 0, .15); border: 1rpx rgba(0, 0, 0, .1) solid; border-radius: 2rpx; margin-left: -1rpx;}
.swiper_wrap .swiper_item{ width: 100%; height: 100%;}
.swiper_wrap .swiper_item .text_body{ top: 0; bottom: 0;}


/* v等级界面 */
.vlevel_con{ display:flex;}
.vlevel_con .part_left{ width: 120rpx;  position: relative; padding: 5rpx; height: 100%; }
.vlevel_con .part_left .part_left_bg{  width: 100%;}
.vlevel_con .part_left .infos{ position:absolute; top: 48%; left: 0; bottom: 0; width: 100%; height: 50%; overflow: hidden; text-align: center; font-size: 12rpx;color: #fff; }
.vlevel_con .part_left .infos .txt{ height: 20rpx; line-height: 20rpx; margin-top: 16rpx;}
.vlevel_con .part_left .infos .number{ height: 24rpx; margin-top: 5rpx; }
.vlevel_con .part_left .infos .icon_v{ width: 24rpx; margin:0 -3rpx;}
.vlevel_con .part_left .infos.change .txt{ margin-top: 0; font-size: 10rpx;}
.vlevel_con .part_left .infos.change .number{ margin-top: 0; }
.vlevel_con .part_left .infos.change .icon_v{ width: 13rpx; margin-left: -1rpx;}
.vlevel_con .part_left .infos .level_change{ padding-top: 18rpx; position: relative;}
.vlevel_con .part_left .infos .level_change .icon_arrow{ position: absolute; top: 3rpx; left: 50%; margin-left: -5rpx; width: 10rpx; height: 10rpx; transform: rotate(-90deg);}
.vlevel_con .part_left .infos .level_change .icon_v{ width: 20rpx;}
.vlevel_con .part_right{ width: 240rpx; box-sizing:border-box; font-size: 12rpx; color: #fff;}
.vlevel_con .part_right .description{ height: 16rpx; line-height: 16rpx; font-size: 9rpx; text-align: center; white-space: nowrap; width: 100%; }
.vlevel_con .part_right_01{ padding: 0 10rpx 0 5rpx; }
.vlevel_con .part_right_01 .description{ height: 20rpx; line-height: 20rpx;}
.vlevel_con .part_right_01 .tr{ display:flex; line-height: 20rpx; height: 20rpx; padding: 0 10rpx; position: relative; }
.vlevel_con .part_right_01 .tr .td{ flex: 1;}
.vlevel_con .part_right_01 .tr .td .icon{  width: 12rpx;}
.vlevel_con .part_right_01 .tr:nth-child(even){background-color:rgba(0, 0, 0, .1)}
.vlevel_con .part_right_01 .tr .cur_arrow{ position: absolute; left: 0; top: 50%; margin-top: -6rpx; width: 8rpx; height: 10rpx; transform:rotate(180deg); display: none;}
.vlevel_con .part_right_01 .tr.light{ color:#e2c041; font-weight: bold;}
.vlevel_con .part_right_01 .tr.light .cur_arrow{ display:inline-block;}
.vlevel_con .part_right_02{  padding: 0 10rpx 5rpx 5rpx;}
.vlevel_con .part_right_02 .description{ height: 20rpx; }
.vlevel_con .part_right_02 .v_slider{ margin: 0; height: 30rpx; position: relative; }
.vlevel_con .part_right_02 .btn_setvlevel{ width: 80rpx; height: 30rpx;line-height: 30rpx; font-size: 12rpx; margin: 0;}


/* 高招返利 弹窗 */
.pop_gift_mask{ background: linear-gradient(0deg, rgba(0,0,0,1) 0%, rgba(0, 0, 0, 0) 100%);z-index: 998; }
.pop_gift_mask.hide{transition-delay: .1s;}
.pop_gift{ position: fixed; width: 100%; height: 120rpx; bottom: 0rpx; top: auto; left: 0; right: 0;transition-duration:.3s; }
.pop_gift.hide{ opacity: 0; transform: scale(1,1); transform: translateY(130rpx);}
.pop_gift .gift_con_bg{  position: absolute; left: 0; right: 0; bottom: 10rpx; top:auto; width: 100%; height: 100rpx;}
.pop_gift .zhezhao{ position: absolute; bottom: 0; height: 100rpx; width: 100rpx; display: none;}
.pop_gift .zhezhao.l{ left: 0; right: auto; background: linear-gradient(90deg, rgba(0,0,0,1) 0%, rgba(0, 0, 0, 0) 100%);}
.pop_gift .zhezhao.r{ right: 0; left: auto; background: linear-gradient(90deg, rgba(0,0,0,0) 0%, rgba(0, 0, 0, 1) 100%);}
.pop_gift .zhezhao.b{ bottom: 0; left: 0; right: 0; width: 100%; height: 10rpx; background: linear-gradient(0deg, rgba(0,0,0,1) 0%, rgba(0, 0, 0, 0) 100%);}
.pop_gift .title_bg_cloud,
.pop_gift .title_bg,
.pop_gift .title_txt{ transform:translateX(40rpx)}
.pop_gift .btn_close{ transform:translateX(-10rpx)}
.pop_gift .title_rank{ position: absolute; left:-5rpx; top: -30rpx; height: 90rpx; transform:rotate(-10deg);}

.pop_gift .nr{ position: absolute; left: 0; right: 0; bottom: 10rpx; top: auto; height: 65rpx; text-align: center;}
.gifts{  display: inline-block; vertical-align: top;}
.gifts .count_num{ font-size: 14rpx; display: inline-block; height: 30rpx; line-height: 50rpx;  padding-right: 20rpx; vertical-align: top; }
.gifts .item{ position: relative; width: 40rpx; margin: 0; overflow: visible; padding: 0;color: #fff; height: auto; margin:0 8rpx; vertical-align: top;}
.gifts .item .icon_gift{ width: 100%; position:static; }
.gifts .item .extra{ position: absolute; left: -10rpx; top: 3rpx; height: 36rpx; width: 20rpx;}
.gifts .item .txt_bg{ position: absolute; top: 33rpx; left: -8rpx; width: 54rpx; opacity: .5;}
.gifts .item .txt{ position: absolute; top: 35rpx; left: 5rpx; white-space: nowrap; font-size: 10rpx; line-height: 20rpx; color: #fff;}
.gifts .item .yiling{ position: absolute; left: -3rpx; top: 10rpx; width: 45rpx; opacity: 0;transform: scale(1.5,1.5); }
.gifts .item.faguang{ animation: faguang2 1s ease-in-out infinite;}
.gifts .item.faguang .icon_gift{ animation: tada .8s ease-in-out infinite; }
.gifts .item.done .icon_gift{ filter:grayscale(1);}
.gifts .item.done .yiling{ opacity: 1; transform: scale(1,1);}


/* 忍者配置 */
.ninjasetting_con{ position: relative; width: 100%; height: 100%; }
.ninjasetting_con .description{ color: #fff;  height: 16rpx; line-height: 16rpx; font-size: 9rpx; text-align: center; white-space: nowrap; }
.ninjasetting_con .part_left{ position: absolute; left: 0; top: 0; bottom: 0; right: 150rpx; height: 100%;  box-sizing: border-box;}
.ninjasetting_con .part_left .description{ text-align: left; padding: 5rpx 10rpx;}
.ninjasetting_con .part_left .text_body{  top: 0rpx; left: 0; right:0; bottom:0; }
.ninjasetting_con .part_left .text_body::before{ content: ''; display: block; width: 100%; height: 20rpx; background:linear-gradient(180deg, rgba(0,0,0,.5) 0%, rgba(0, 0, 0, 0) 100%); position: absolute; top: 0; left: 0; right: 0; z-index: 1;}
.ninjasetting_con .part_left .text_body::after{ content: ''; display: block; width: 100%; height: 20rpx; background:linear-gradient(0deg, rgba(0,0,0,.5) 0%, rgba(0, 0, 0, 0) 100%); position: absolute; bottom: 0; left: 0; right: 0; z-index: 1;}
.ninja_selectable_list{ line-height: 0; padding:5rpx 0 20rpx; z-index: 0; }
.ninja_selectable_list .item_wrap{display:inline-block; width: 50rpx; margin: 0 0 0 2rpx; animation-duration:.2s; animation-timing-function: ease-in-out; }
.ninja_selectable_list .item{ width: 50rpx; height:50rpx; position: relative; margin: 0; padding: 0; background-color: transparent; border-radius: 0; padding: 0; overflow: visible; transform: scale(.8);  }
.ninja_selectable_list .btn_active{ transform: scale(.7);}
.ninja_selectable_list .item .image_bg{ position:absolute; left: 0; right: 0; top: 0; bottom: 0; width: 100%; height: 100%;}
.ninja_selectable_list .item .img{ display: block; width: 100%; height: 100%; background-repeat: no-repeat; background-size: 2400rpx 240rpx; position: absolute; left: 0; top: 0; }
.ninja_selectable_list .item .fg{display: block; width: 100%; height: 100%; position: absolute; left: 0; top: 0;  }
.ninja_selectable_list .item .fg .image_fg{ position:absolute; left: 0; right: 0; top: 0; bottom: 0; width: 100%; height: 100%;}
.ninja_selectable_list .item .fg .image_rank{ display: block; position: absolute; left: -1rpx; top: -1rpx; width: 20rpx; height: 22rpx; }
.ninja_selectable_list .item .fg .num{ display: block; position: absolute; right: 2rpx; bottom: 2rpx; font-size: 12rpx; font-weight: bold;}
.ninja_selectable_list .text{ display: block; height: 14rpx; width: 100rpx; margin-left: -25rpx; font-weight: light; font-size: 12rpx;line-height: 14rpx; text-align: center; white-space: nowrap; }
.ninja_selectable_list .item .fg .light{ position: absolute;  left: -2rpx; right: -2rpx; top: -2rpx; bottom: -2rpx; width: 54rpx; height: 54rpx; display: none; mix-blend-mode:screen; }
.ninja_selectable_list .item .ninja_active{ position: absolute;  left: -2rpx; right: -2rpx; top: -2rpx; bottom: -2rpx; width: 54rpx; height: 54rpx; display: none; }
.ninja_selectable_list .item .corner_tag{ position: absolute; right: -10rpx; top: -6rpx; width: 25rpx; height: 35rpx; display: none;} 
.ninja_selectable_list .item_wrap.now .item{animation: faguang 2s ease-in-out infinite;}
.ninja_selectable_list .item_wrap.now .light{ display: block;}
.ninja_selectable_list .item_wrap.cur .ninja_active{ display: none;}
.ninja_selectable_list .item_wrap.cur .corner_tag{ display: block;}
.ninja_selectable_list .text_l{ transform: scale(0.9,0.9);}
.ninja_selectable_list .text_name{ color:#ea9237; display: none;}

.ninjasetting_con .part_right{ position: absolute; right: 0; top: 0; width: 150rpx; height: 100%; background-color: rgba(0, 0, 0, 0.1); border-left: 1rpx solid rgba(0, 0, 0, .2); }
.ninjasetting_con .part_right .tabs{ width: 100%; padding:0; margin-bottom: 10rpx; text-align: left;}
.ninjasetting_con .part_right .tabs .tab_item{ margin: 0;  padding: 0; width: 100%; height: 70rpx;  overflow: visible;}
.ninjasetting_con .part_right .tabs .tab_item .btn_bg{ position: absolute; left: 0; right: 0; top: 0; bottom: 0; width: calc(100% + 15rpx); height: 100%; margin-left: -16rpx;}
.ninjasetting_con .part_right .tabs .tab_item .btn_bg_cur{ display: none;}
.ninjasetting_con .part_right .tabs .tab_item .arrow{ position: absolute; left: -8rpx; top: 50%; margin-top: -8rpx; width: 18rpx; display: none;}
.ninjasetting_con .part_right .tabs .tab_item .text{ font-size: 14rpx; line-height: 40rpx; height: 40rpx; position: absolute; left: auto; right: 0; text-align:left;}
.ninjasetting_con .part_right .tabs .tab_item .text .image_rank{ width: 30rpx; vertical-align: middle; margin-top: -3rpx ;}
.ninjasetting_con .part_right .tabs .tab_item .icon{ position:absolute; left: 5rpx; right: auto; top: 50%; margin-top: -25rpx; transform: scale(.8);}
.ninjasetting_con .part_right .tabs .tab_item .icon .fg .image_rank{ display: block; position: absolute; left: -1rpx; top: -1rpx; width: 20rpx; height: 22rpx; }
.ninjasetting_con .part_right .tabs .tab_item .info{ position: absolute; top: 14rpx; right: 0; left: 55rpx; bottom: auto; text-align: left; font-size: 10rpx; height: 16rpx; line-height: 14rpx; text-shadow:1rpx 1rpx 1rpx #111,-1rpx -1rpx 1rpx #111, 0 2rpx 1rpx #111, 2rpx 0 1rpx #111, -2rpx 0 1rpx #111, 0 -2rpx 1rpx #111;}
.ninjasetting_con .part_right .tabs .tab_item .info .bt{ font-size: 11rpx; margin-bottom: 5rpx;}
.ninjasetting_con .part_right .tabs .current{ color: #fff;}
.ninjasetting_con .part_right .tabs .current .btn_bg_def{ display: none; }
.ninjasetting_con .part_right .tabs .current .btn_bg_cur{ display: block; }
.ninjasetting_con .part_right .part_right_2{ position: absolute; bottom: 10rpx; width: 100%;  padding-top: 5rpx;}
.ninjasetting_con .btn_setninja{ width: 80rpx; height: 30rpx;line-height: 30rpx; font-size: 12rpx; margin: 0;}


