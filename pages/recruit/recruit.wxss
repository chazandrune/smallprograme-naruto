page,view,image{box-sizing: border-box;}
page{ width: 100%; height: 100%; position: relative; background-color: #000; color: #fff; overflow-y: hidden;display: flex; align-items: center; 
    -webkit-text-size-adjust:none; /*解决了IOS文字大小不正常问题*/
}

/* 重写checkbox样式 */
switch{ font-size: 14rpx; }
switch .wx-checkbox-input { display: none;}
switch .wx-checkbox-input.wx-checkbox-input-checked { background: red;}
switch .wx-checkbox-input.wx-checkbox-input-checked::before { color:green;}
switch .icon{ width: 28rpx; height: 28rpx;}
switch .icon.on{display:none;}
switch[checked] .icon.off { display: none;}
switch[checked] .icon.on { display: inline-block;}
switch .txt{ display: inline-block; white-space: nowrap; padding: 0 3rpx; }

/* 重写button样式 */
button::after{ border: none; background-color: transparent;}
button[disabled]{ background-color: transparent; color: #fffd78;}
button[disabled]:not([type]){ background-color: transparent;color: #fffd78;}

/* 重写滑动条样式 */
wx-slider .wx-slider-handle-wrapper{ height: 10rpx; border-radius: 1rpx; border:3rpx #370000 solid;}
wx-slider .wx-slider-tap-area{ padding: 8rpx 0;}
wx-slider .wx-slider-track{background: linear-gradient(to right, #de4f00 0%, #ff9c00 100%); border-radius: 1rpx;}
wx-slider .wx-slider-thumb{ height: 20rpx !important; width: 20rpx !important; margin-top: -10rpx !important; margin-left:-10rpx !important; background:linear-gradient(to right, #de4f00 0%, #ff9c00 100%); border: 2rpx #a2320c solid; box-sizing: border-box; box-shadow: 0 2rpx 3rpx rgba(43, 12, 12, 0.5);}



/* 动画组 */
@keyframes load1 {0% {transform: rotate(0deg);}100% {transform: rotate(360deg);}}
@keyframes faguang{
    0% { box-shadow: 0 0 5rpx rgba(255,185,62,.8);filter: brightness(1.5);}
    50% {  box-shadow: 0 0 20rpx rgba(255,185,62,.3);filter: brightness(1); }
    100% {  box-shadow: 0 0 50rpx rgba(255,185,62,.8);filter: brightness(1.5); }
}
@keyframes faguang2{
    0% { filter: brightness(2);}
    50% { filter: brightness(1); }
    100% { filter: brightness(2); }
}
@keyframes tada{
    0%
    {transform:scale3d(1,1,1)}
    10%,20%
    {transform:scale3d(.9,.9,.9) rotate3d(0,0,1,-3deg)}
    30%,50%,70%,90%
    {transform:scale3d(1.1,1.1,1.1) rotate3d(0,0,1,3deg)}
    40%,60%,80%
    {transform:scale3d(1.1,1.1,1.1) rotate3d(0,0,1,-3deg)}
    100%
    {transform:scale3d(1,1,1)}
}
@keyframes yanwu{
0% { background-position: 0 0; }
6.25% { background-position: -40rpx 0; }
22.25% { background-position: -80rpx 0; }
28.5% { background-position: -120rpx 0; }
34.75% { background-position: 0 -40rpx; }
41% { background-position: -40rpx -40rpx; }
47.25% { background-position: -80rpx -40rpx; }
53.5% { background-position:-120rpx  -40rpx; }
59.75% { background-position: 0 -80rpx; }
66% { background-position: -40rpx -80rpx; }
72.25% { background-position: -80rpx -80rpx; }
78.5% { background-position: -120rpx -80rpx; }
84.75% { background-position: 0 -120rpx; }
91% { background-position: -40rpx -120rpx; }
97.25% { background-position: -80rpx -120rpx; }
100% { background-position: -120rpx -120rpx; }
}
@keyframes fadeIn{0%{opacity:0}100%{opacity:1}}
@keyframes fadeScaleIn{0%{opacity:0;transform:scale(0);}100%{opacity:1;transform:scale(1);}}
@keyframes fadeInLeft{0%{opacity:0;transform:translate3d(-100%,0,0)}100%{opacity:1;transform:none}}
@keyframes fadeInDown{0%{opacity:0;transform:translateY(100%)}100%{opacity:1;transform:translateY(0)}}
@keyframes fudong{0%{transform:translateY(0)}50%{transform:translateY(5%)}100%{transform:translateY(0)}}
@keyframes zhishi{0%{transform:translateX(-3rpx)}50%{transform:translateX(3rpx)}100%{transform:translateX(-3rpx)}}
@keyframes fadeInBig{0%{opacity:0;transform:scale(10);filter: blur(4rpx);}100%{opacity:1;transform:scale(1);filter: blur(0rpx);}}
@keyframes fadeOutBig{0%{opacity:0;transform:scale(1)}10%{opacity:.8;transform:scale(1)}50%{opacity:0;transform:scale(1.2)}100%{opacity:0;transform:scale(1.2)}}
@keyframes xuanzhuan{0%{transform:rotate(0)}50%{transform:rotate(180deg)}100%{transform:rotate(360deg)}}

.animated{animation-duration:.5s;animation-fill-mode:both}
.fadeIn{animation-name:fadeIn;}
.fadeScaleIn{animation-name:fadeScaleIn;}
.fadeInLeft{animation-name:fadeInLeft;}
.fadeInDown{animation-name:fadeInDown; animation-timing-function:ease-out;}
.fadeInBig{animation-name:fadeInBig; animation-timing-function:cubic-bezier(.86,0,.62,1.27); animation-duration:.7s;animation-fill-mode:both;animation-delay: 500ms;}
.fadeOutBig{animation-name:fadeOutBig; animation-timing-function:ease-out; animation-iteration-count:infinite;animation-duration:3s;animation-fill-mode:both; }
.fudong{animation-name:fudong; animation-timing-function:ease-in-out; animation-iteration-count:infinite;animation-duration:5s;animation-fill-mode:both;}

